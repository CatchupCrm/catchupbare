{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alnutile/l5scaffold.git"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.x-dev",
    "ext-gmp": "*",
    "ext-gd": "*",
    "turbo124/laravel-push-notification": "^2.0",
    "doctrine/dbal": "^2.3",
    "maddhatter/laravel-fullcalendar": "^1.2",
    "laravelcollective/html": "^5.2",
    "patricktalmadge/bootstrapper": "^5.5.0",
    "anahkiasen/former": "^4.0.5",
    "intervention/image": "dev-master",
    "guzzlehttp/guzzle": "^6.2",
    "wildbit/laravel-postmark-provider": "^3.0",
    "laravel/socialite": "^2.0",
    "laracasts/presenter": "dev-master",
    "maatwebsite/excel": "^2.0",
    "ezyang/htmlpurifier": "^v4.7",
    "cerdic/css-tidy": "^v1.5",
    "barryvdh/laravel-ide-helper": "^2.2",
    "barryvdh/laravel-debugbar": "^2.2",
    "fzaninotto/faker": "^1.5",
    "jaybizzle/laravel-crawler-detect": "^1.0",
    "codedge/laravel-selfupdater": "5.x-dev",
    "predis/predis": "^1.1",
    "nwidart/laravel-modules": "~1.16",
    "jonnyw/php-phantomjs": "4.*",
    "php-ews/php-ews": "dev-master",
    "cviebrock/eloquent-taggable": "^2.0",
    "cviebrock/eloquent-sluggable": "^4.1",
    "caffeinated/modules": "^4.1",
    "wikimedia/composer-merge-plugin": "^1.3",
    "filp/whoops": "^2.1"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "CalendarDemo\\": "app/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ]
  }
}
